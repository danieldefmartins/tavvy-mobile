{
  "expo": {
    "name": "Tavvy",
    "slug": "tavvy-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0F1233"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Tavvy to use your location to show nearby places."
        }
      ],
      "@maplibre/maplibre-react-native"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.360.tavvy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "Tavvy needs your location to show nearby places on the map.",
        "NSLocationAlwaysUsageDescription": "Tavvy needs your location to show nearby places.",
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F1233"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.danieldefmartins.tavvymobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2ef919c9-1f62-4e75-9b4d-485e74b7c676"
      }
    }
  }
}
